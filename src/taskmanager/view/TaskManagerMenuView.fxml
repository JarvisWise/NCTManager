<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="taskmanager.controller.TaskManagerMenuController">
    <children>
        <Button fx:id="addTaskButton" layoutX="14.0" layoutY="26.0" mnemonicParsing="false" onAction="#onClickedAddTask" prefHeight="25.0" prefWidth="86.0" text="Add Task" />
        <Button fx:id="calendarButton" layoutX="115.0" layoutY="26.0" mnemonicParsing="false" onAction="#onClickedCalendar" prefHeight="25.0" prefWidth="92.0" text="Calendar" />
      <ListView fx:id="taskList" layoutX="16.0" layoutY="67.0" onMouseClicked="#onClickEditTask" prefHeight="316.0" prefWidth="571.0" />
    </children>
</AnchorPane>
